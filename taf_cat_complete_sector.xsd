																<xs:fractionDigits value="1"/>
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element ref="MaxSpeedEmpty">
								<xs:annotation>
									<xs:documentation>Maxumum speed conditions empty arising rom wagon construction characteristics (in km/h)</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element ref="AirBrake"/>
